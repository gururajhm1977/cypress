{
    "name": "cypress-project",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "build": "ng build --prod --build-optimizer=true",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "e2ecy": "cypress run",
        "cy:run": "cypress run",
        "e2e:headless": "cypress run",
        "cy:open": "./node_modules/.bin/cypress open",
        "e2e:chrome": "cypress run --browser chrome",
        "e2e:record": "cypress run --record",
        "cy:parallel": "cypress-parallel -s cy:run -t 2 -d cypress/integration",
        "e2e:api": "node ./cypress/apiContractTest.js",
        "qa:smoke:scripts": "cypress run --spec cypress/integration/*.js --env ENV=qa --browser chrome",
        "stage:smoke:scripts": "cypress run --spec cypress/integration/*.js --env ENV=staging --browser chrome",
        "prod:smoke:scripts": "cypress run --spec cypress/integration/*.js --env ENV=production --browser chrome",
        "allureReport": "cypress run --spec cypress/integration/*.js --env ENV=qa --browser chrome allure=true,allureResultsPath=allure-report"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/marmelab/gremlins.js"
    },
    "private": true,
    "dependencies": {
        "@currents/cli": "^1.0.8",
        "cypress-axe": "^0.14.0",
        "cypress-image-snapshot": "^4.0.1",
        "cypress-parallel": "^0.1.8",
        "cypress-tags": "^0.3.0",
        "cypress-tesults-reporter": "^1.2.0",
        "gremlins.js": "^2.2.0",
        "jquery": "^3.5.1",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@bahmutov/cy-api": "^2.1.2",
        "@shelex/cypress-allure-plugin": "^2.26.5",
        "axe-core": "^4.4.1",
        "codelyzer": "^6.0.0",
        "cypress": "^9.5.1",
        "cypress-audit": "^1.1.0",
        "cypress-terminal-report": "^3.4.2",
        "newman": "^5.2.2",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.2"
    }
}
